<% form_for(@member) do |f| %>
    <%= f.error_messages %>

    <p>
      <%= f.label :employee_id %><br/>
      <%= f.text_field :employee_id %>
    </p>
    <p>
      <%= f.label :name %><br/>
      <%= f.text_field :name %>
    </p>
    <p>
      <%= f.label :nickname %> <br/>
      <%= f.text_field :nickname %>
    </p>
    <p>
      <%= f.label :contact_number %><br/>
      <%= f.text_field :contact_number %>
    </p>
    <p>
      <%= f.label :email %><br/>
      <%= f.text_field :email %>
    </p>

    <p>
      <%= f.label :admin %> <%= f.check_box  :admin?%>
    </p>

    <p>
      <%= f.submit 'Save' %>
    </p>
<% end %>